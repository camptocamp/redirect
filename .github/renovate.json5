{
  extends: [
    'github>camptocamp/gs-renovate-config-preset:base.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:group.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:ci.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:preset.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:pre-commit.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:python.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:docker.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:json-schema.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:shellcheck.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:stabilization-branches.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:own.json5#1.4.1',
    'github>camptocamp/gs-renovate-config-preset:security.json5#1.4.1',
  ],
  baseBranches: ['1.0', '1.1', 'master'],
  packageRules: [
    /** Docker images versions */
    {
      matchDatasources: ['docker'],
      versioning: 'ubuntu',
      allowedVersions: '/^[0-9][02468]\\.04$/',
      matchDepNames: ['ubuntu'],
    },
    /** Disable upgrading the supported Python version */
    {
      matchFileNames: ['pyproject.toml'],
      enabled: false,
      matchDepNames: ['python'],
    },
    /** Packages published very recently are not pushed to stabilization branches for security reasons */
    {
      matchBaseBranches: ['/^[0-9]+\\.[0-9]+$/'],
      minimumReleaseAge: '7 days',
    },
  ],
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
}
